<%= require 'digest' %>

<?php


$i=0;

<% @servers.each do |server| -%>
$i++;
  <% server.each do |property, value| -%>
$cfg['Servers'][$i]['<%= property %>'] = '<%= value %>';
  <% end -%>
<% end %>

$cfg['blowfish_secret'] = '<%= Digest::SHA256.hexdigest @hostname %>';

<% @settings.each do |key, val| %>
$cfg['<%= key %>'] = '<%= val %>';
<% end -%>
[A
?>
